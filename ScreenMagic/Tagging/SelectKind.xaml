<Window x:Class="ScreenMagic.SelectKindWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ScreenMagic"
        mc:Ignorable="d"
        Title="Select Kind" Height="248" Width="170.406">
    <Grid Margin="0,0,5,0.5">
        <ListBox x:Name="listBoxKinds" HorizontalAlignment="Left" Height="174" VerticalAlignment="Top" Width="158"/>
        <Button x:Name="buttonOk" Content="Ok" HorizontalAlignment="Left" Margin="104,188,0,0" VerticalAlignment="Top" Width="34" RenderTransformOrigin="2.227,0.425" Click="ButtonOk_Click" IsDefault="True"/>
        <Button x:Name="buttonCancel" Content="Cancel" HorizontalAlignment="Left" Margin="10,188,0,0" VerticalAlignment="Top" Width="75" Click="ButtonCancel_Click"/>

    </Grid>
</Window>
